{
	"name": "notification/signature",
	"type": "wordpress-plugin",
	"scripts": {
		"phpcs": "phpcs",
		"phpcbf": "phpcbf"
	},
	"autoload": {
		"psr-4": {
			"BracketSpace\\Notification\\Signature\\": "src/classes"
		},
		"files": [
			"src/includes/functions/general.php"
		]
	},
	"require": {
		"typisttech/imposter-plugin": "^0.6.2",
		"micropackage/requirements": "^1.0",
		"micropackage/dochooks": "^1.0",
		"micropackage/filesystem": "^1.1"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
		"phpcompatibility/php-compatibility": "^9.1",
		"wp-coding-standards/wpcs": "^2.0"
	},
	"extra": {
		"imposter": {
			"namespace": "BracketSpace\\Notification\\Signature\\Vendor"
		}
	}
}
